# Copy this file to .env and fill in values locally.
# IMPORTANT: Do not commit .env

# Canonical timezone for stored/canonical timestamps
DATA_TZ=UTC

# Presentation timezone for reports
REPORT_TZ=Europe/Vienna

# Provider keys
# - keep secrets out of Git (do NOT commit .env)
FRED_API_KEY=

# LLM Configuration (OpenAI compatible)
# API Key (OpenAI or OpenRouter)
OPENAI_API_KEY=

# Optional: Custom Base URL (e.g. for OpenRouter)
# LLM_BASE_URL=https://openrouter.ai/api/v1

# Optional: Models to use (comma-separated)
# Default: gpt-4o
# LLM_MODELS=gpt-4o,anthropic/claude-3.5-sonnet
